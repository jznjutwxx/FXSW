        <div id="fundForm">
            <el-row type="flex">
                <div class="blueTitle"></div>
                <div class="titlecss">资金配套组成</div>
            </el-row>
            <el-row type="flex" class="rowpadding">
                <el-col v-bind:span="8" class="colheight">
                    <el-form-item label="市补:">
                        <el-input v-model="editForm.N_SUBSIDY_CITY">
