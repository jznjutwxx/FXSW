        mounted: function () {
            this.getTown("/FarmWaterOutside/GetDictionary", { type: "街镇" });
            this.getStatics("/FarmWaterOutside/GetDictionary", { type: "工程状态" });
            this.getLegalPerson("/FarmWaterOutside/GetLegalPerson", null);
            this.getDesignUnit("/FarmWaterOutside/GetDesignUnit", null);

            $(window).resize(function () {
                this.gccteditmapheight = document.documentElement.clientHeight - 200;
            });
        },
