            gccteditmapheight: function (val) {
                this.gccteditmapheight = val;
            },
        },
        methods: {
            setDraftStatus: function (param) {
                this.addForm.N_DRAFT = param.n_draft;
            },
            bindGCCTData:function(){
                this.gcctinfo.S_PROJECT_NO = this.addForm.S_PROJECT_NO;
                this.gcctinfo.N_PACE_STATUS = this.addForm.N_PACE_STATUS;
                this.gcctinfo.N_YEAR = this.addForm.N_YEAR;
                this.gcctinfo.N_TYPE = this.addForm.N_TYPE;
                this.gcctinfo.S_LEGAL_PERSON = this.addForm.S_LEGAL_PERSON;
                this.gcctinfo.S_TOWN = this.addForm.S_TOWN;
                this.gcctinfo.S_ADDRESS = this.addForm.S_ADDRESS;
                this.gcctinfo.S_UNIT_DESIGN = this.addForm.S_UNIT_DESIGN;
                this.gcctinfo.S_UNIT_BUILD = this.addForm.S_UNIT_BUILD;
                this.gcctinfo.S_UNIT_SUPERVISE = this.addForm.S_UNIT_SUPERVISE;
                this.gcctinfo.N_RECKON_TOTAL_AMT = this.addForm.N_RECKON_TOTAL_AMT;
                this.gcctinfo.REMARK = this.addForm.S_REMARK;
            },
            showgcctMapAdd: function () {
                this.bindGCCTData();
                this.mapAddDialogVisible = true;
            },
            getYear: function (url, type) {
                axios.post(url, type)
                  .then(function (response) {
                      var tempData = JSON.parse(response.data);
                      var datas = tempData.DictionaryGetResponse.list;
                      this.YearOption = datas;
                  }.bind(this))
                  .catch(function (error) {
                      console.log(error);
                  });
            },
            getTown: function (url, type) {
                axios.post(url, type)
                  .then(function (response) {
                      var tempData = JSON.parse(response.data);
                      var datas = tempData.DictionaryGetResponse.list;
                      this.xzqhOptions = datas;
                      this.addForm.S_TOWN = this.xzqhOptions[0].s_cd_value;

                  }.bind(this))
                  .catch(function (error) {
                      console.log(error);
                  });
            },
            getStatics: function (url, type) {
                axios.post(url, type)
                  .then(function (response) {
                      var tempData = JSON.parse(response.data);
                      var datas = tempData.DictionaryGetResponse.list;
                      this.staticsOptions = datas;
                      this.addForm.N_PACE_STATUS = this.staticsOptions[0].s_cd_key;
                  }.bind(this))
                  .catch(function (error) {
                      console.log(error);
                  });
            },
            getLegalPerson: function (url, param) {
                axios.post(url, param)
                  .then(function (response) {
                      var tempData = JSON.parse(response.data);
                      var datas = tempData.EnginLegalPersonListGetResponse.engin_unit_person_list;
                      this.LegalPerOption = datas;
                      if ('@type' == "person") {
                          this.isPersonDisable = true;
                          this.isUnitDisable = false;
                          var selectPerson = "";
                          selectPerson = this.LegalPerOption.filter(function (item) {
                              if (item.s_account == '@token') {
                                  return item.s_id;
                              }
                          });
                          this.addForm.S_LEGAL_PERSON = selectPerson.length > 0 ? selectPerson[0].s_id : ""
                      }
                  }.bind(this))
                  .catch(function (error) {
                      console.log(error);
                  });
            },
            getDesignUnit: function (url, param) {
                axios.post(url, param)
                  .then(function (response) {
                      var tempData = JSON.parse(response.data);
                      var datas = tempData.EnginUnitListGetResponse.engin_unit_person_list;
                      this.DesignUnitOption = datas;
                      if ('@type' == "unit") {
                          this.isPersonDisable = false;
                          this.isUnitDisable = true;

                          var selectUnit = "";
                          selectUnit = this.DesignUnitOption.filter(function (item) {
                              if (item.s_account == '@token') {
                                  return item.s_id;
                              }
                          });
                          this.addForm.S_UNIT_DESIGN = selectUnit.length > 0 ? selectUnit[0].s_id : ""
                      }
                  }.bind(this))
                  .catch(function (error) {
                      console.log(error);
                  });
            },
            changeYear: function (param) {
                this.addForm.N_YEAR = param;
            },
            changeStatics: function (param) {
                this.N_PACE_STATUS = param;
            },
            changeXzqh: function (param) {
                this.S_TOWN = param;
            },
            changeLegalper: function (param) {
                this.S_LEGAL_PERSON = param;
            },
            changeDesignUnit: function (param) {
                this.S_UNIT_DESIGN = param;
            },
            cancel: function () {
                window.location.href = "/FarmWaterOutside/Index";
            },
            save: function (formName) {
                this.addForm.N_YEAR = new Date(this.addForm.N_YEAR).getFullYear();
                this.$refs[formName].validate(function (valid) {
                    if (valid) {
                        var drawing = '';
                        var picture = '';
                        if (this.drawingFiles != '')
                        {
                            for (var i = 0; i < this.drawingFiles.length; i++)
                            {
                                drawing = this.drawingFiles[i].name + ',' + drawing
                            }
                        }
                        if (this.pictureFiles != '') {
                            for (var i = 0; i < this.pictureFiles.length; i++) {
                                picture = this.pictureFiles[i].name + ',' + picture
                            }
                        }
                        axios.post('/FarmWaterOutside/Add',
                            { param: JSON.stringify(this.addForm), drawingFiles: drawing, pictureFiles: picture })
                          .then(function (response) {
                              var datas = JSON.parse(response.data);
                              var tempR = datas.EnginFarmReportResponse.result;
                              this.addForm.N_YEAR = moment().format('YYYY');
                              if (tempR) {
                                  window.location.href = "/FarmWaterOutside/Index";
                              } else {
                                  this.$message.error('保存失败！' + TempR);
                              }
                          }.bind(this))
                          .catch(function (error) {
                              this.addForm.N_YEAR = moment().format('YYYY');
                              console.log(error);
                          });
                    } else {
                        this.addForm.N_YEAR = moment().format('YYYY');
                        console.log('error submit!!');
                        return false;
                    }
                }.bind(this));
            },
            beforeAvatarUpload: function (file) {
                var fileType = file.name.substring(file.name.lastIndexOf('.') + 1);
                if (fileType == 'exe') {
                    this.$message({
                        message: '不支持上传该类型文件！',
                        type: 'warning'
                    });
                    return false;
                }
                const isLt2M = file.size / 1024 / 1024 < 10
                if (!isLt2M) {
                    this.$message({
                        message: '上传文件大小不能超过 10MB!',
                        type: 'warning'
                    });
                    return false;
                }
            },
            beforeImagesUpload: function (file) {
                var fileType = file.name.substring(file.name.lastIndexOf('.') + 1);
                //image/png, image/jpg, image/jpeg, image/gif
                if (fileType != 'png' && fileType != 'jpg' && fileType != 'jpeg' && fileType != 'gif') {
                    this.$message({
                        message: '不支持上传该类型文件！',
                        type: 'warning'
                    });
                    return false;
                }
                const isLt2M = file.size / 1024 / 1024 < 10
                if (!isLt2M) {
                    this.$message({
                        message: '上传文件大小不能超过 10MB!',
                        type: 'warning'
                    });
                    return false;
                }
            },
            beforeRemove: function (file, fileList) {
                if (file.status == 'ready') {
                    return;
                }
                return this.$confirm('此操作将永久删除该文件, 是否继续?', '提示', {
                    confirmButtonText: '确定',
                    cancelButtonText: '取消',
                    type: 'warning'
                }).then(function () {
                    axios.post('/FarmlandWater/DeleteFilesLocal', { filename: file.name })
                  .then(function (response) {
                      if (response.data == "OK") {
                          this.$message({
                              message: '删除成功！',
                              type: 'success'
                          });
                      } else {
                          this.$message.error('删除失败！' + response.data);
                      }
                  }.bind(this))
                  .catch(function (error) {
                      console.log(error);
                  });
                }.bind(this)).catch(function () {
                    this.$message({
                        type: 'info',
                        message: '已取消删除！'
                    });
                });

            },
            handleRemove: function (file, fileList) {
                console.log(file, fileList);
            },
            getDrawing: function (file, fileList) {
                this.drawingFiles = fileList;
                //console.log(this.file);
            },
            getPicture: function (file, fileList) {
                this.pictureFiles = fileList;
                //console.log(this.file);
            },
            handlePreview: function (file) {
                //console.log(file);
                this.dialogImageUrl = file.url;
                this.dialogVisible = true;
            },
            upSucess: function (response, file, fileList) {
                this.$message({
                    message: '上传成功！',
                    type: 'success'
                });
            },
            upError: function (err, file, fileList) {
                this.$message.error('上传失败！' + err);
            },
            openIndex:function(){
                window.location.href = "/FarmWaterOutside/Index";
            }
        }
    });
</script>

