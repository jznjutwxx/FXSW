                                   v-bind:on-change="getPicture"
                                   v-bind:on-success="upSucess"
                                   v-bind:on-error="upError"
                                   v-bind:before-remove="beforeRemove"
                                   v-bind:before-upload="beforeImagesUpload"
                                   accept="image/png, image/jpg, image/jpeg, image/gif">
